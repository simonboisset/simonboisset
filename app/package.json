{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npx prisma generate --data-proxy && tailwindcss -o ./app/styles/tailwind.css && remix build",
    "dev": "concurrently \"tailwindcss -o ./app/styles/tailwind.css --watch\" \"cross-env NODE_ENV=development netlify dev\" \"tsc --noEmit --watch\"",
    "start": "cross-env NODE_ENV=production netlify dev",
    "story": "concurrently \"tailwindcss -o ./app/styles/tailwind.css --watch\" \"ladle serve\"",
    "kill": "docker stop $(docker ps -aq) && docker rm $(docker ps -aq)",
    "up": "yarn upgrade-interactive --latest",
    "setup": "prisma generate dev",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "reset": "prisma migrate reset --force",
    "docker": "docker-compose up -d",
    "seed": "prisma db seed",
    "ts": "tsc --noEmit",
    "test": "NODE_ENV=production yarn jest",
    "restart": "yarn kill && yarn docker && yarn setup && yarn dev"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.2.0",
    "@headlessui/react": "^1.6.5",
    "@lexical/code": "^0.3.5",
    "@lexical/link": "^0.3.5",
    "@lexical/list": "^0.3.5",
    "@lexical/markdown": "^0.3.5",
    "@lexical/react": "^0.3.5",
    "@lexical/rich-text": "^0.3.5",
    "@lexical/selection": "^0.3.5",
    "@lexical/table": "^0.3.5",
    "@lexical/utils": "^0.3.5",
    "@netlify/functions": "^1.0.0",
    "@popperjs/core": "^2.11.5",
    "@prisma/client": "3.15.2",
    "@remix-run/netlify": "^1.6.1",
    "@remix-run/node": "^1.6.1",
    "@remix-run/react": "^1.6.1",
    "@ts-v/core": "^0.2.2",
    "@ts-v/kit": "^0.2.2",
    "@ts-v/remix": "^0.2.2",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.3",
    "lexical": "^0.3.5",
    "markdown-it": "^13.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.1",
    "@remix-run/eslint-config": "^1.6.1",
    "@swc/core": "^1.2.204",
    "@swc/jest": "^0.2.21",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^28.1.3",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "concurrently": "^7.2.2",
    "eslint": "^8.15.0",
    "jest": "^28.1.1",
    "prisma": "^3.15.2",
    "remix-flat-routes": "^0.4.3",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}

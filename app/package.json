{
  "private": true,
  "name": "@simonboisset/website",
  "description": "",
  "license": "",
  "scripts": {
    "build": "yarn build:css && remix build",
    "preview": "yarn build:css && netlify build --context deploy-preview && netlify deploy",
    "deploy": "yarn build:css && netlify build && netlify deploy --prod",
    "test": "jest",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "dev": "concurrently \"tailwindcss -o ./app/styles/tailwind.css --watch\" \"cross-env NODE_ENV=development netlify dev\" \"tsc --noEmit --watch\"",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@netlify/functions": "^1.0.0",
    "@notionhq/client": "^1.0.4",
    "@popperjs/core": "^2.11.5",
    "@remix-run/netlify": "^1.4.1",
    "@remix-run/react": "^1.4.1",
    "dayjs": "^1.11.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@remix-run/dev": "^1.4.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "babel-jest": "^27.5.1",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "jest": "^27.5.1",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
